#include <stdio.h>

int main()
{
    int num,i,fact;
    printf("enter any number:");
    scanf("%d",&num);
    fact=i=1;
    
    while(i<=num)
      {
          fact=fact*i;
          i++;
      }
    printf("fact of any number is %d",fact);

    return 0;
}


//Time complexity - O(n)
